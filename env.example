# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/ai_data_science_platform
DATABASE_URL_DEV=sqlite:///./ai_data_science_dev.db

# Redis Configuration (for Celery and caching)
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# API Keys (copy from fetch/ai-data-science/.env)
OPENAI_API_KEY=sk-proj...
AGENTVERSE_API_TOKEN=eyJhbGc...

# Application Settings
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# File Storage
UPLOAD_PATH=./uploads
MAX_FILE_SIZE_MB=100
ALLOWED_EXTENSIONS=.csv,.xlsx,.json,.parquet,.pdf

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=ai_data_science_platform

# H2O AutoML Configuration
H2O_NTHREADS=-1
H2O_MAX_MEM_SIZE=4g

# Application Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=8001 